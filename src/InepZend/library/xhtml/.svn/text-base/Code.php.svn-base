<?php
namespace xhtml;
/**
 * The HTML Code Element (<code>) represents a fragment of computer code.
 *
 * By default, it is displayed in the browser's default monospace font.
 *
 * @note {
 *  A CSS rule can be defined for the code selector to override the browser's
 *  default font face. Preferences set by the user might take precedence over
 *  the specified CSS.
 * }
 *
 * @note {
 *  Use the cite attribute on a \<blockquote> or \<q> element to reference an
 *  online resource for a source.
 * }
 *
 * @example {
 *  <p>Regular text. <code>This is code.</code> Regular text.</p>
 * }
 *
 * @link http://htmldog.com/reference/htmltags/code/
 * @link http://developer.mozilla.org/en/HTML/Element/code
 */
class Code extends abstractEntity\TextEntity
{
    /**
     * XML Tag Name
     *
     * @var string
     */
    protected $strTag = 'code';
}
?>