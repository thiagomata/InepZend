<?php
namespace xhtml;
/**
 * The HTML Map element (\<map>) is used with \<area> elements to define a image map.
 *
 * @example {
 *   <map>
 *      <area shape="circle" coords="200,250,25" href="another.htm" />
 *      <area shape="default" />
 *   </map>
 * }
 *
 * @link http://www.w3.org/TR/html5/the-map-element.html#the-map-element
 * @link http://htmldog.com/reference/htmltags/map/
 * @link http://developer.mozilla.org/en/HTML/Element/map/
 */
class Map extends abstractEntity\HtmlEntity
{
    /**
     * XML Tag Name
     *
     * @var string
     */
    protected $strTag = 'map';

    /**
     * Child Nodes Rules
     *
     * @var String[]
     */
    protected $arrChildNodesRule = array(
        'area' => '*'
    );
}
?>